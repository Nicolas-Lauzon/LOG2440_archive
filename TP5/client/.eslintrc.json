{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["plugin:react/recommended", "airbnb"],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "plugins": ["react"],
  "rules": {
    "jsx-a11y/label-has-associated-control": [
      "error",
      {
        "required": {
          "some": ["nesting", "id"]
        }
      }
    ],
    "jsx-a11y/label-has-for": [
      "error",
      {
        "required": {
          "some": ["nesting", "id"]
        }
      }
    ],
    "jsx-quotes": 0,
    "object-curly-newline": 0,
    "react/jsx-one-expression-per-line": 0,
    "linebreak-style": ["error", "windows"],
    "react/no-unescaped-entities": 0,
    "react/prop-types": 0,
    "no-plusplus": 0,
    "max-len": [
      "error",
      {
        "code": 120,
        "comments": 130,
        "ignoreStrings": true,
        "ignoreUrls": true,
        "ignoreTemplateLiterals": true
      }
    ],
    "class-methods-use-this": 0,
    "no-restricted-syntax": 0,
    "no-await-in-loop": 0,
    "consistent-return": 0,
    "no-unused-vars": "error",
    "no-var": 1
  }
}
